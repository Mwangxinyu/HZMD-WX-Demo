.sz-empty {
    position: absolute;
    z-index: 1;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    display: flex;
    flex-direction: column;
    align-items: center;
    background-color: transparent;
}

.sz-empty image {
    margin-top: 300rpx;
    width: 262rpx;
    height: 265rpx;
}

.sz-empty text {
    margin-top: 30rpx;
    font-size: 26rpx;
    color: #BBB;
    line-height: 37rpx;
}